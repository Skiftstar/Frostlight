{
	"general": {
		"sidebar": {
			"monitor": 0
		},
		"topbar": {
			"enabled": true
		}
	},
	"customization": {
		"wallust": {
			"enabled": true,
			"command": "wallust run '{{INPUT}}' -k -s -C '{{CONFIG}}'"
		},
		"wallpaper": {
			"command": "swww img '{{INPUT}}'"
		},
		"imageManipulation": {
			"enabled": true,
			"command": "magick '{{INPUT}}' -brightness-contrast -40x0 '{{OUTPUT}}'"
		}
	}
}