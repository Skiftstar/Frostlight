{
	"general": {
		"sidebar": {
			"monitor": "1"
		},
		"topbar": {
			"enabled": true
		}
	},
	"overlay": {
		"applauncher": {
			"commands": "ags,htop,vesktop"
		}
	},
	"customization": {
		"wallust": {
			"enabled": true,
			"command": "wallust run '{{INPUT}}' -k -s -C '{{CONFIG}}'"
		},
		"wallpaper": {
			"command": "swww img '{{INPUT}}'"
		},
		"imageManipulation": {
			"enabled": true,
			"command": "magick '{{INPUT}}' -brightness-contrast -10x0 '{{OUTPUT}}'"
		}
	}
}